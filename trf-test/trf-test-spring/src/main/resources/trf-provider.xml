<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:trf="http://www.test.com/schema/trf"
          xsi:schemaLocation="
	        http://www.springframework.org/schema/beans
	        http://www.springframework.org/schema/beans/spring-beans.xsd
	        http://www.test.com/schema/trf
            http://www.test.com/schema/trf.xsd ">

    <trf:application id="test-application" name="test-application"/>

    <trf:registry id="test-registry" protocol="zookeeper_tujia" address="N/A" group="trf" />

    <trf:protocol id="test-protocol" name="trf" port="20777"/>

    <trf:provider id="test-provider" executes="200" threads="200" token="true" timeout="3000"/>


</beans>