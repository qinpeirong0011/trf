<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:trf="http://www.test.com/schema/trf"
          xsi:schemaLocation="
	        http://www.springframework.org/schema/beans
	        http://www.springframework.org/schema/beans/spring-beans.xsd
	        http://www.test.com/schema/trf
            http://www.test.com/schema/trf.xsd ">

    <trf:application id="test-application" name="test-application"/>

    <trf:registry id="test-registry" protocol="zookeeper_tujia" address="127.0.0.1:2181" group="trf" />

    <trf:protocol id="test-protocol" name="trf" port="20777"/>

    <trf:provider id="test-provider" executes="200" threads="200" token="true" timeout="3000"/>

    <bean id="demoServiceTest" class="com.qinpr.trf.demo.provider.DemoServiceImpl"/>

    <trf:service id="test-service" interface="com.qinpr.trf.demo.DemoService" ref="demoServiceTest" version="1.0" protocol="test-protocol" group="test-group"/>

</beans>